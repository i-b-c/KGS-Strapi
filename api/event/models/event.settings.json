{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name_et": {
      "type": "string"
    },
    "slug_et": {
      "type": "uid",
      "targetField": "name_et"
    },
    "name_en": {
      "type": "string"
    },
    "slug_en": {
      "type": "uid",
      "targetField": "name_en"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "program",
        "residency",
        "festival",
        "tour",
        "project"
      ]
    },
    "hide_from_page": {
      "type": "boolean"
    },
    "canceled": {
      "type": "boolean"
    },
    "conversation": {
      "type": "boolean"
    },
    "ticket_info": {
      "model": "ticket-info"
    },
    "start_time": {
      "type": "datetime"
    },
    "end_time": {
      "type": "datetime"
    },
    "duration": {
      "type": "integer"
    },
    "subtitle_et": {
      "type": "string"
    },
    "subtitle_en": {
      "type": "string"
    },
    "description_et": {
      "type": "richtext"
    },
    "description_en": {
      "type": "richtext"
    },
    "technical_info_et": {
      "type": "richtext"
    },
    "technical_info_en": {
      "type": "richtext"
    },
    "order": {
      "type": "integer"
    },
    "online": {
      "type": "string"
    },
    "video": {
      "type": "richtext"
    },
    "audio": {
      "type": "richtext"
    },
    "remote_id": {
      "type": "uid"
    },
    "resident": {
      "type": "string"
    },
    "location_et": {
      "type": "string"
    },
    "location_en": {
      "type": "string"
    },
    "X_ticket_info": {
      "type": "component",
      "repeatable": false,
      "component": "ticket-info.x-ticket-info"
    },
    "performance": {
      "via": "events",
      "model": "performance"
    }
  }
}
